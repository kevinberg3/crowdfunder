  <h1>Edit <%= @project.title %> </h1>
 
  <%= form_for @project, :url => my_project_path(@project.id) do |f| %> 

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :teaser %>
    <%= f.text_field :teaser %> 

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.label :goal %>
    <%= f.text_field :goal %></br></br>
    <%= f.submit "Update Project" %> 

    
 
  <% end %> 